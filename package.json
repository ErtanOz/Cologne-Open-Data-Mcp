{
  "name": "cologne-open-data-mcp",
  "version": "0.1.0",
  "description": "Model Context Protocol (MCP) server providing access to Cologne's Open Data APIs",
  "type": "module",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "bin": {
    "cologne-open-data-mcp": "dist/server.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "scripts": {
    "dev": "tsx src/server.ts",
    "dev:sse": "tsx src/server-sse.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:sse": "node dist/server-sse.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cologne",
    "koeln",
    "open-data",
    "api",
    "parking",
    "kvb",
    "nextbike",
    "oparl",
    "rhein",
    "water-level"
  ],
  "author": "Ertan Oz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ErtanOz/Cologne-Open-Data-Mcp.git"
  },
  "homepage": "https://github.com/ErtanOz/Cologne-Open-Data-Mcp#readme",
  "bugs": {
    "url": "https://github.com/ErtanOz/Cologne-Open-Data-Mcp/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.5.0",
    "undici": "^6.19.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^24.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3"
  }
}
